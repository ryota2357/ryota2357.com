{
  "name": "ryota2357.com",
  "packageManager": "pnpm@8.7.4",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "prettier --write '**/*.{astro,ts,js,cjs,json,yaml,md}'",
    "format:check": "prettier --check '**/*.{astro,ts,js,cjs,json,yaml,md}'",
    "typecheck": "astro check && tsc --noEmit",
    "lint": "eslint --fix './{src,plugins}/**/*.{astro,ts}'",
    "lint:check": "eslint './{src,plugins}/**/*.{astro,ts}'",
    "textlint": "textlint --fix './src/content/**/*.md'",
