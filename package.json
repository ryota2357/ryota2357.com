{
  "name": "ryota2357.com",
  "packageManager": "pnpm@8.7.0",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "prettier --write '**/*.{astro,ts,tsx,json,yaml,md}'",
    "format:check": "prettier --check '**/*.{astro,ts,tsx,json,yaml,md}'",
    "typecheck": "astro check && tsc --noEmit",
    "lint": "eslint --fix './src/**/*.{astro,ts,tsx}'",
    "lint:check": "eslint './src/**/*.{astro,ts,tsx}'",
    "textlint": "textlint --fix './content/**/*.md'",
    "textlint:check": "textlint --dry-run './content/**/*.md'",
    "astro": "astro",
    "preinstall": "npx -y only-allow pnpm"
  },
  "dependencies": {
    "@astrojs/rss": "3.0.0",
    "@astrojs/sitemap": "3.0.0",
    "@astrojs/tailwind": "5.0.0",
    "astro": "3.0.3",
    "dayjs": "1.11.9",
    "destyle.css": "4.0.0",
    "sass": "1.66.1",
    "sharp": "0.32.5",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.48.0",
    "eslint-plugin-astro": "0.29.0",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.0",
    "prettier-plugin-tailwindcss": "0.5.4",
    "textlint": "13.3.3",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-preset-ja-spacing": "2.3.0",
    "textlint-rule-preset-ja-technical-writing": "8.0.0",
    "typescript": "5.2.2"
  },
  "volta": {
    "node": "20.5.1"
  }
}
